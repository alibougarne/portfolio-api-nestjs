{
   "type": "sqlite",
   "database": "database.sqlite",
   "synchronize": false,
   "logging": true,
   "entities": [
      "dist/**/**.entity{.ts,.js}",
      "dist/**/shared/entities/**.entity{.ts,.js}"
   ],
   "migrations": [
      "dist/**/shared/migration/*{.ts,.js}"
   ],
   "subscribers": [
      "src/subscriber/**/*.ts"
   ],
   "cli": {
      "entitiesDir": "src/shared/entity",
      "migrationsDir": "src/shared/migration",
      "subscribersDir": "src/subscriber"
   }
}